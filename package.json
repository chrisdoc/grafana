{
  "name": "grafana-foundation-dashboards",
  "version": "0.1.0",
  "description": "Grafana dashboards built with TypeScript and Foundation SDK",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.17",
    "pnpm": ">=10",
    "npm": "please-use-pnpm"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "tsx build.ts",
    "generate": "pnpm run build",
    "upload": "./upload.sh",
    "deploy": "pnpm run build && pnpm run upload",
    "clean": "rm -rf dist output build",
    "fmt": "biome format --write .",
    "lint": "biome lint --write .",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [
    "grafana",
    "dashboard",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grafana/grafana-foundation-sdk": "11.6.0-cogv0.0.x.1753977659"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/node": "^24.10.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
